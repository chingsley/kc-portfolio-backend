  "jest": {
    "collectCoverage": false,
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/build/"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/",
      "<rootDir>/src/utils/",
      "<rootDir>/test/",
      "<rootDir>/src/server.js",
      "<rootDir>/src/database/models/index.js"
    ]
  },
  "lint-staged": {
    "src/**/*.{js}": [
      "eslint . --fix",
      "npm run test"
    ]
  },
  "husky": {
    "hooks": {
      "lint": "npm run lint ",
      "pre-commit": "npm run coverage"
    }
  },
  "directories": {
    "test": "test"
  }
